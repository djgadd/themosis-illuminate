{
  "name": "keltiecochrane/themosis-illuminate",
  "description": "A themosis plugin that implements the illuminate/cache packing with a WP_Object_Cache driver",
  "keywords": ["themosis", "plugin"],
  "authors": [
    {
      "name": "Daniel Gadd",
      "email": "daniel@djgadd.uk",
      "homepage": "http://djgadd.uk"
    },
    {
      "name": "Keltie Cochrane Ltd.",
      "email": "digital@keltiecochrane.com",
      "homepage": "https://keltiecochrane.com"
    }
  ],
  "type": "wordpress-plugin",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=7.0",
    "composer/installers": "~1.2",
    "illuminate/support": "^5.4",
    "illuminate/filesystem": "^5.4",
    "illuminate/validation": "^5.4",
    "illuminate/translation": "^5.4",
    "illuminate/mail": "^5.4",
    "illuminate/config": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "KeltieCochrane\\Illuminate\\": "./src/"
    },
    "files": [
      "./src/helpers.php"
    ]
  }
}
